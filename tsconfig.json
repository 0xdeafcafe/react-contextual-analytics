{
	"extends": "@tsconfig/recommended",
	"compilerOptions": {
		"incremental": true,
		"target": "ES2020",
		"lib": ["ES2020", "DOM"],
		"module": "ESNext",
		"declaration": true,
		"sourceMap": true,
		"composite": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx"
	}
}
